{
  "name": "beepboopbot",
  "version": "1.0.0",
  "description": "A fun way to learn programming concepts, Help B3 get home",
  "engines": {
    "node": ">= 22"
  },
  "scripts": {
    "start": "cd client && NODE_ENV=development webpack-dev-server --hot --inline",
    "build": "npm run build:js && npm run build:styles",
    "build:styles": "sass --load-path scss client/scss/main.scss public/resources/styles/style.css",
    "watch:styles": "nodemon -e scss -x \"npm run build:styles\"",
    "build:js": "cd client && webpack --progress --profile --colors",
    "watch:js": "cd client && webpack --progress --profile --colors --watch",
    "postbuild": "mkdir -p webpage/ && cp -r public/* webpage/ && node postbuild.mjs",
    "lint": "npx standard ./client",
    "lint:fix": "npx standard --fix ./client",
    "test": "babel-tape-runner client/test/*.js | tap-spec"
  },
  "license": "MIT",
  "dependencies": {
    "classnames": "^2.2.5",
    "react": "^15.2.1",
    "react-cookie": "^0.4.7",
    "react-dom": "^15.2.1",
    "react-motion": "^0.4.4",
    "react-redux": "^4.4.5",
    "react-skylight": "^0.4.0",
    "react-sound": "^0.5.1",
    "redux": "^3.5.2",
    "redux-sounds": "^1.1.0",
    "redux-thunk": "^2.1.0"
  },
  "devDependencies": {
    "@types/node": "^25.0.3",
    "babel-loader": "^6.2.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.11.1",
    "babel-preset-stage-0": "^6.5.0",
    "babel-tape-runner": "^2.0.1",
    "deep-freeze": "0.0.1",
    "redux-logger": "^2.6.1",
    "sass-embedded": "^1.87.0",
    "tap-spec": "^4.1.1",
    "tape": "^4.6.0",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  }
}
